<Window x:Class="Projet1.TrouverPartenaire"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projet1"
        mc:Ignorable="d"
        Title="TrouverPartenaire" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black" Offset="0"/>
                <GradientStop Color="#FFF20303" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Button Content="Precedent" HorizontalAlignment="Left" Margin="686,34,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.5,0.5" Background="#FFFDFF00" FontFamily="Segoe UI Black" Click="Precedent"/>
        <Image HorizontalAlignment="Left" Height="324" Margin="440,76,0,0" VerticalAlignment="Top" Width="306" Source="729453.png"/>
        <Label Content="Quel est votre classement : " HorizontalAlignment="Left" Margin="33,68,0,0" VerticalAlignment="Top" Height="30" Width="159" Foreground="White"/>
        <TextBox x:Name="classement" HorizontalAlignment="Left" Height="23" Margin="201,68,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="55"/>
        <TextBlock x:Name="trouver" HorizontalAlignment="Left" Margin="10,228,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="192" Width="407" Foreground="White"/>
        <Label Content="Uniquement pour les joueurs de compétition" HorizontalAlignment="Left" Margin="33,31,0,0" VerticalAlignment="Top" Height="30" Width="306" Foreground="White"/>
        <Button Content="Chercher" HorizontalAlignment="Left" Margin="339,68,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.5,0.5" Background="#FFFDFF00" FontFamily="Segoe UI Black" Click="Chercher"/>
        <Label HorizontalAlignment="Left" Margin="33,103,0,0" VerticalAlignment="Top" Height="30" Width="39" Foreground="White"/>
        <TextBox x:Name="jour" HorizontalAlignment="Left" Height="23" Margin="71,146,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="55"/>
        <Label Content="Mois" HorizontalAlignment="Left" Margin="146,146,0,0" VerticalAlignment="Top" Height="26" Width="40" Foreground="White"/>
        <TextBox x:Name="mois" HorizontalAlignment="Left" Height="23" Margin="191,146,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="55"/>
        <Label Content="Année" HorizontalAlignment="Left" Margin="251,139,0,0" VerticalAlignment="Top" Height="30" Width="43" Foreground="White" RenderTransformOrigin="0.628,1.15"/>
        <TextBox x:Name="annee" HorizontalAlignment="Left" Height="23" Margin="325,146,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="55"/>
        <Label Content="Jour" HorizontalAlignment="Left" Margin="26,146,0,0" VerticalAlignment="Top" Height="26" Width="40" Foreground="White"/>
        <Label Content="Date de naissance" HorizontalAlignment="Left" Margin="33,107,0,0" VerticalAlignment="Top" Height="26" Width="118" Foreground="White"/>

    </Grid>
</Window>
